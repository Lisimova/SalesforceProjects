<aura:component description="OutputField" controller="ExchangeRatesController">
    <aura:attribute name="object" type="Exchange_Rate__c" />
    <aura:attribute name="field" type="String" />
    <aura:attribute name="fieldVal" type="String" access="private" />
    <aura:attribute name="baseCurrency" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    <div class="slds-text-body--regular">
        {!v.fieldVal}
    </div>
</aura:component>